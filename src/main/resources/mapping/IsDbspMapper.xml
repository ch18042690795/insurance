  <insert id="insertSelective" parameterType="com.middle.entity.IsDbsp">
    insert into is_dbsp
    <trim prefix="(" suffix=")" suffixOverrides=",">
      <if test="pkid != null">
        pkid,
      </if>
      <if test="sbcwpc != null">
        sbcwpc,
      </if>
      <if test="jzlx00 != null">
        jzlx00,
