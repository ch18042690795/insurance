    insert into is_dbsp (pkid, sbcwpc, jzlx00,fwwdbh,fzxbh0,isFileup
      )
    values (#{pkid,jdbcType=VARCHAR}, #{sbcwpc,jdbcType=VARCHAR}, #{jzlx00,jdbcType=VARCHAR}
    , #{fwwdbh,jdbcType=VARCHAR},#{fzxbh0,jdbcType=VARCHAR},0
      )
